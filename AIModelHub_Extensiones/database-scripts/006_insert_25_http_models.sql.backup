-- ============================================================================
-- VERIFICATION QUERY
-- ============================================================================

SELECT 
    COUNT(*) as total_models,
    COUNT(*) FILTER (WHERE da.type = 'HttpData') as http_models
FROM assets a
JOIN data_addresses da ON a.id = da.asset_id
WHERE a.id LIKE 'asset-vision-%' 
   OR a.id LIKE 'asset-nlp-%'
   OR a.id LIKE 'asset-health-%'
   OR a.id LIKE 'asset-flora-%'
   OR a.id LIKE 'asset-fraud-%';
